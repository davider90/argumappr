{
  "name": "master-project",
  "version": "0.1.0",
  "description": "This is the master project repository",
  "keywords": [],
  "author": "David Ferenc Bendiksen <davidbendiksen@hotmail.com>",
  "license": "MIT",
  "types": "./lib/cjs/types/index.d.ts",
  "main": "./lib/cjs/index.js",
  "scripts": {
    "test": "mocha --config ./configs/.mocharc.json",
    "clean": "rm -rf ./lib",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p ./configs/tsconfig.esm.json && mv lib/esm/index.js lib/esm/index.mjs",
    "build:cjs": "tsc -p ./configs/tsconfig.cjs.json",
    "prepack": "npm run build",
    "document": "typedoc --options ./configs/typedoc.json"
  },
  "files": [
    "lib/**/*"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/types/index.d.ts",
        "default": "./lib/esm/index.mjs"
      },
      "require": {
        "types": "./lib/cjs/types/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "chai": "^4.3.7",
    "eslint": "^8.37.0",
    "mocha": "^10.2.0",
    "prettier": "^2.8.2",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.24",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@types/graphlib": "^2.1.8",
    "graphlib": "^2.1.8"
  },
  "prettier": {},
  "eslintConfig": {
    "extends": [
      "./configs/.eslintrc.json"
    ]
  }
}
